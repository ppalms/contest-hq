<%
  @show_copy_prompt = false
%>
<%= turbo_stream.replace "music_slot_prescribed" do %>
  <% slot = { 
    type: :prescribed, 
    prescribed_music: @prescribed_music,
    position: @position,
    is_new: true
  } %>
  <%= render "music_selections/prescribed_slot", slot: slot, contest_entry: @contest_entry %>
<% end %>
